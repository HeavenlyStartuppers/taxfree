{
  "name": "tax-free",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "size": "npm run build && size-limit",
    "start": "next start",
    "lint": "next lint"
  },
  "size-limit": [
    {
      "name": "pages",
      "limit": "100 kB",
      "path": ".next/static/chunks/pages/*.js"
    },
    {
      "name": "framework",
      "limit": "100 kB",
      "path": ".next/static/chunks/framework-*.js"
    },
    {
      "name": "main",
      "limit": "100 kB",
      "path": ".next/static/chunks/main-*.js"
    },
    {
      "name": "functions",
      "limit": "100 kB",
      "path": ".next/serverless/pages/api/*.js"
    }
  ],
  "dependencies": {
    "@headlessui/react": "^1.3.0",
    "@panelbear/panelbear-js": "^1.2.0",
    "@pwabuilder/pwainstall": "^1.6.7",
    "@revolut/icons": "^1.37.0",
    "@revolut/ui-kit": "^11.15.1",
    "next": "11.0.1",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.2.23",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0",
    "use-pwa": "^1.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^11.0.1",
    "@size-limit/file": "^5.0.1",
    "@types/node": "^16.3.1",
    "@types/react": "17.0.13",
    "autoprefixer": "^10.2.6",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "postcss": "^8.3.5",
    "size-limit": "^5.0.1",
    "tailwindcss": "^2.2.4",
    "typescript": "4.3.5"
  }
}
