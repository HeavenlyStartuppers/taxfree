{
  "name": "tax-free",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "size": "npm run build && size-limit",
    "start": "next start",
    "lint": "next lint"
  },
  "size-limit": [
    {
      "name": "pages",
      "limit": "100 kB",
      "path": ".next/static/chunks/pages/*.js"
    },
    {
      "name": "framework",
      "limit": "100 kB",
      "path": ".next/static/chunks/framework-*.js"
    },
    {
      "name": "main",
      "limit": "100 kB",
      "path": ".next/static/chunks/main-*.js"
    }
  ],
  "dependencies": {
    "@panelbear/panelbear-js": "^1.3.2",
    "next": "^12.0.2",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.2",
    "@size-limit/file": "^6.0.4",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "autoprefixer": "^10.4.0",
    "eslint": "7.30.0",
    "eslint-config-next": "12.0.2",
    "postcss": "^8.3.11",
    "size-limit": "^6.0.4",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.4.4"
  }
}
